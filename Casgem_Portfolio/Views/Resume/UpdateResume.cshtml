@model Casgem_Portfolio.Models.Entities.TblResume
@{
    ViewBag.Title = "UpdateResume";
    Layout = "~/Views/Shared/_PortfolioAdminLayout.cshtml";
}

<h2>Öz Geçmiş Sayfası</h2>
<form method="post">
    @Html.HiddenFor(x => x.ResumeID)
    <label>Üst Başlık</label>
    @Html.TextBoxFor(x => x.Title1, new { @class = "form-control" })
    <br />
    <label>Alt Başlık</label>
    @Html.TextBoxFor(x => x.Title2, new { @class = "form-control" })
    <br />
    <label>İçerik</label>
    @Html.TextBoxFor(x => x.Descreption, new { @class = "form-control" })
    <br />

    <button class="btn btn-success">Güncelle</button>
</form>